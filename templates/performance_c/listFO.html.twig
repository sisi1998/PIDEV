<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.5.0/Chart.min.js"></script>
<section style=' margin: auto; width:100%; padding: 10px'>
 
<canvas id="myChart1" style="width:100%;max-width:600px;  margin-left: auto;
    margin-right: auto;"></canvas>


<script>
var data = {{ data|json_encode()|raw }};

 var xValues = ["Buts","Carton Rouge","Carton Jaune","Passes décisives","AériensGagnés","Tirs par match"];
var yValues = Object.values(data);
var barColors = ["red", "green","blue","orange","brown","orange"];

new Chart("myChart1", {
  type: "bar",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    legend: {display: false},
    title: {
      display: true,
      text: "Nos Statiques des Performances des joueurs"
    }
  }
});
</script>


