<script>
    $(document).ready(function () {
        // Update the event listener to watch for changes on both input fields
        $("#searchCompetition").keyup(function (e) {
            // Get the value of the competition input field
            var competitionValue = $("#searchCompetition").val();

            $.ajax({
                url: "{{ path('ajax_search') }}",
                type: 'GET',
                data: {
                    // Send only the competition name to the server
                    'competition': competitionValue
                },
                success: function (resultats) {
                    if (resultats) {
                        $('#t tbody#searchtab').empty();
                        $.each(resultats, function (i, obj) {
                            $('#t tbody#all').hide();
                            $('#t tbody#searchtab').append('<tr><td> ' + obj.id + '</td><td> ' + obj.Buts + '</td><td> ' + obj.Rouge + '</td><td> ' + obj.Jaune );
                        });
                    } else {
                        $('#t tbody#all').show();
                        $('#t tbody#searchtab').empty();
                        $('#t tbody#searchtab').fadeIn('fast');
                    }
                }
            });
            return false;
        });
    });
</script>

            
